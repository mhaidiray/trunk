        <servlet-name>SearchManager</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonalDataManager</servlet-name>
        <url-pattern>/persinfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeAdminManager</servlet-name>
        <url-pattern>/adminhome</url-pattern>
    </servlet-mapping>
