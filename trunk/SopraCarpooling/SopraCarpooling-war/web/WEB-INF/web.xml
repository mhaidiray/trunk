        <servlet-name>UserEditManager</servlet-name>
        <servlet-class>Controller.UserEditManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlaceEditManager</servlet-name>
        <servlet-class>Controller.PlaceEditManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GestroutesManager</servlet-name>
        <servlet-class>Controller.GestroutesManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConnectManager</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountCreateManager</servlet-name>
        <url-pattern>/newcpt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetPwdManager</servlet-name>
        <url-pattern>/resetpwd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeUserManager</servlet-name>
