    <servlet>
        <servlet-name>AccountCreateManager</servlet-name>
        <servlet-class>Controller.AccountCreateManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResetPwdManager</servlet-name>
        <servlet-class>Controller.ResetPwdManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeUserManager</servlet-name>
        <servlet-class>Controller.HomeUserManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchManager</servlet-name>
        <servlet-class>Controller.SearchManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonalDataManager</servlet-name>
